<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.astraapi.repository.SpecializationRepository">
    <insert id="save" parameterType="com.example.astraapi.entity.SpecializationEntity" useGeneratedKeys="true" keyProperty="entity.id">
        insert into public.specializations (title) values (#{entity.title})
    </insert>
    <select id="getAll" resultType="com.example.astraapi.entity.SpecializationEntity">
        select * from public.specializations
    </select>
</mapper>
