<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.astraapi.repository.TestExamRepository">
    <insert id="save">
        insert into public.tests_exams(test_id, exam_id) values
        <foreach collection="examIds" item="examId" separator=",">
            (#{testId}, #{examId})
        </foreach>
    </insert>
</mapper>
